<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.bcsoft.careergraph.dao.StepDAO">
    <select id="selectAll" resultMap="stepResultMap">
        SELECT * FROM step
    </select>

    <resultMap id="stepResultMap" type="net.bcsoft.careergraph.entity.Step">
        <id property="id" column="id"/>
        <result property="roadmapId" column="roadmap_id"/>
        <result property="ord" column="ord"/>
        <result property="title" column="title"/>
        <result property="description" column="description"/>
    </resultMap>
</mapper>