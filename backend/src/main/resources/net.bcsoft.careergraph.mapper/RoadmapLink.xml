<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.bcsoft.careergraph.dao.RoadmapLinkDAO">

    <select id="selectAll" resultMap="roadmapLinkResultMap">
        SELECT * FROM roadmap_link
    </select>

    <resultMap id="roadmapLinkResultMap" type="net.bcsoft.careergraph.entity.RoadmapLink">
        <id property="id" column="id"/>
        <result property="idRoadmap" column="roadmap_id"/>
        <result property="idStep" column="step_id"/>
    </resultMap>


</mapper>